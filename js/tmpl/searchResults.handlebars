
{{#each items}}
    <li class="SearchResults-item">
        <a href="{{link}}">
            <h3 class="SearchResults-subTitle">{{title}}</h3>
            <p class="SearchResults-text">{{snippet}}</p>
        </a>
        {{#ifEquals displayLink "community.cloud66.com"}}
        <div class="Tag Tag--lrg">Community</div>
        {{/ifEquals}}
    </li>
{{/each}}


